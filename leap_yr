#include <iostream>

int main() {
int y = 0; // where y is year
std::cout << " Enter the year: \n";
std::cin >> y;
if (y<1000 || y> 9999){
  std::cout << " invalid entry\n";
}  
  else if (y % 4 == 0 && y % 100 != 0 || y % 400 == 0 ){
    std::cout << "Falls on leap year.\n";
  }
  else {
    std::cout << "Normal year\n";
  }
}
